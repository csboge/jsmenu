.page-contianer {
    background: linear-gradient(to bottom right, #9d7094, #4d3b60 17%, #262541 35%, #16111b 60%, #725478 100%);
}

.txt-w {
    color: #fff;
}

.page-head {
    padding-top: 88rpx;
    padding-bottom: 50rpx;
}

button {
    margin-left: 40rpx;
    width: 40rpx;
    height: 36rpx;
    border: 0;
    background: transparent;
    padding-left: 0;
    padding-right: 0;
    line-height: 1;
}

.button-hover {
    background: transparent;
}

button::after {
    border: 0;
}

.share-icon {
    width: 40rpx;
    height: 36rpx;
}

.person-img {
    width: 156rpx;
    height: 156rpx;
    border-radius: 50%;
    border: 4rpx solid #fff;
}

.person-box {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.person-name-box {
    display: flex;
    align-items: center;
    margin-top: 26rpx;
    margin-bottom: 20rpx;
}

.section-nav {
    display: flex;
}

.reduce-item {
    flex-grow: 1;
    padding-top: 14rpx;
    padding-bottom: 8rpx;
    text-align: center;
    background: #23213e;
}

.active {
    background: #604a6d;
}

.tab-vedio {
    padding: 25rpx 36rpx;
}

.vedio-des {
    position: relative;
    margin-top: 30rpx;
    padding: 30rpx 30rpx;
    background: #351e31;
    border-radius: 8rpx;
    overflow: visible;
}

.vedio-arrow {
    position: absolute;
    left: 42rpx;
    top: -40rpx;
    border-left: 20rpx solid transparent;
    border-right: 20rpx solid transparent;
    border-top: 20rpx solid transparent;
    border-bottom: 20rpx solid #351e31;
}

.vedio-des-txt {
    text-align: center;
    line-height: 1.5;
}

video {
    width: 678rpx;
}

.tab-deeds{
    padding: 50rpx 0;
}

.deeds-box{
    display: flex;
    justify-content: center;
}

.dees-circle{
