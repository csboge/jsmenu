    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
    width: 606rpx;
    height: 606rpx;
    background: transparent;
    border-radius: 50%;
    border: 4rpx solid #554364;
}

.inner-circle{
    display: flex;
