.page-head {
    position: relative;
    height: 395rpx;
}

.head-bg {
    position: absolute;
    width: 100%;
    height: 395rpx;
}

.tel-icon {
    position: relative;
    top: 6rpx;
    width: 40rpx;
    height: 40rpx;
}

.share-icon {
    width: 40rpx;
    height: 40rpx;
}

.person-head-box {
    position: relative;
    display: flex;
    justify-content: space-between;
    width: 340rpx;
}

.head-img {
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    width: 126rpx;
    height: 126rpx;
    border-radius: 50%;
    border: 4rpx solid #fff;
    box-sizing: border-box;
}

button {
    position: absolute;
    right: 0;
    bottom: -40rpx;
    display: inline-block;
    margin-right: 0;
    border: 0;
    background: transparent;
    padding-left: 0;
    padding-right: 0;
    line-height: 0;
    padding: 30rpx 30rpx;
}

button::after {
    width: 0;
    border: 0;
}

.button-hover {
    background: transparent;
}

.page-section {
    padding-top: 90rpx;
    width: 100%;
}

.join-us {
    display: flex;
    flex-direction: column;
}

.join-title-img {
    width: 230rpx;
    height: 50rpx;
}

.join-us {
    padding: 30rpx 35rpx;
}

.join-title {
    display: flex;
    justify-content: center;
}

.join-img {
    width: 330rpx;
    height: 190rpx;
    border: 4rpx solid #534964;
    border-radius: 6rpx;
}

.join-item-box {
    display: flex;
    justify-content: space-between;
}

.join-item {
    display: flex;
    flex-direction: column;
    text-align: center;
}

.join-des {
    display: flex;
