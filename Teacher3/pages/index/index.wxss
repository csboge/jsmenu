}

.des-box::after {
    position: absolute;
    content: "";
    left: 50%;
    margin-left: -20rpx;
    top: -24rpx;
    width: 40rpx;
    height: 40rpx;
    background: #fff;
    transform: rotate(45deg);
    border-left: 4rpx solid #604a6d;
    border-top: 4rpx solid #604a6d;
}

