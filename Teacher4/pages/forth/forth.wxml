<view class="page-container">
    <image class='bg-img' src='{{data.content[0]}}' ></image>
    <view style='clear:both;'></view>
    <view class='pro'>
        <image class='pro-title' src='../../assets/images/prd_title.png' ></image>
        <swiper indicator-dots="{{true}}" autoplay="{{true}}" interval="{{5000}}" duration="{{700}}">
            <block wx:for="{{data.banner}}">
                <swiper-item>
                    <image src="{{item}}" class="slide-image" />
                </swiper-item>
            </block>
        </swiper>
    </view>
    <image class='bg-img1' src='{{data.content[1]}}' ></image>
    <image class='bg-img2' src='{{data.content[2]}}' ></image>
    <view style='clear:both;'></view>
    <view class='video-box'>
        <video class='video' src='http://img.my-shop.cc/video/tc4_nj_v.mp4?1'
            controls poster=''></video>
    </view>
    <view class='blank'></view>
</view>