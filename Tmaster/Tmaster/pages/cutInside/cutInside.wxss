

.cropper{
    position: absolute;
    top: 0;
    left: 0;
}

.cropper-buttons{
    background-color: rgba(0, 0, 0, 0.95);
    color: #04b00f;
}
.cropper-buttons{
    position: fixed;
    bottom: 0;
    display: flex;
    justify-content: space-between;
    width: 100%;
    height: 100rpx;
}
.btn{
    display: flex;
    align-items: center;
    width: 50%;
    height: 100rpx;
}

.upload{
    display: flex;
    align-items: center;
    padding-left: 30rpx;
}

.getCropperImage{
    display: flex;
    align-items: center;
    padding-right: 30rpx;
}